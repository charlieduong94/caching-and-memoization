{
  "name": "caching-and-memoization",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "docker-compose build",
    "dev": "docker-compose up web db redis",
    "stop": "docker-compose down",
    "test": "bash scripts/test.sh",
    "remove": "docker-compose rm",
    "dev:server": "npm run server",
    "build:server": "node build/build.js",
    "server": "nodemon build/dev-server.js",
    "pub:sub": "node playground/pubSub.js",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --fix --ext .js,.vue .",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && rm -rf _book && gitbook build",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:publish": "npm run docs:build && cd _book && git init && git commit --allow-empty -m 'Update docs' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'Update docs' && git push git@github.com:Code-Craftsmanship-Saturdays/caching-and-memoization gh-pages --force"
  },
