    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "comments": false,
    "env": {
      "test": {
        "presets": [
          "env",
          "stage-2"
        ],
        "plugins": [
          "istanbul"
        ]
      }
    }
